<h1>Add New Patient</h1>
<hr />
<%= form_with(model: @patient, local: true, class: "ui form") do |f| %>


<div class = "field">
  <%= f.label :first_name %>
  <%= f.text_field :first_name, required: true, autofocus: true, placeholder: "Patient's First Name" %>
</div>
<div class = "field">
  <%= f.label :last_name %>
  <%= f.text_field :last_name, required: true, placeholder: "Patient's Last Name" %>
</div>
<%=f.submit "Add Patient", class: "ui button green inverted" %>
<%end%>